{
  "name": "very-simple-components",
  "version": "0.2.1",
  "description": "A very simple way to attach javascript to the DOM",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "shx rm -rf ./dist && tsup src/index.ts src/useRouter.ts --dts --format esm",
    "dev": "vite",
    "prepublishOnly": "npm run build",
    "release": "np --no-2fa",
    "test": "vitest run",
    "test-dev": "vitest watch"
  },
  "keywords": [
    "components",
    "vanilla",
    "simple",
    "DOM",
    "attach",
    "reuse"
  ],
  "author": "arnoson",
  "license": "MIT",
  "devDependencies": {
    "jsdom": "^19.0.0",
    "np": "^7.6.1",
    "shx": "^0.3.4",
    "tsup": "^6.0.1",
    "vite": "^2.9.9",
    "vitest": "^0.13.1"
  }
}
